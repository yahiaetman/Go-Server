    "version": "0.2.0",
    "configurations": [
        {
            "name": "Console-Go",
			"type": "node",
			"request": "launch",
			"args": ["${workspaceRoot}/src/examples/console-go.ts"],
			"runtimeArgs": ["-r", "ts-node/register"],
			"cwd": "${workspaceRoot}",
			"protocol": "inspector",
            "internalConsoleOptions": "openOnSessionStart",
            "console": "integratedTerminal",
			"env": {
				"TS_NODE_IGNORE": "false"
			}
        }
    ]
}